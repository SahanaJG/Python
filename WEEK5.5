def expand_string(input_str):
    expanded_str = ""
    i = 0
    while i < len(input_str):
        char = input_str[i]
        if char.isalpha():
            count = 0
            while i + 1 < len(input_str) and input_str[i + 1].isdigit():
                count = count * 10 + int(input_str[i + 1])
                i += 1
            expanded_str += char * count
        i += 1
    return expanded_str

input_str = input()

output_str = expand_string(input_str)
print(output_str)
