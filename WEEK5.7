t=int(input())
n=0
while(n!=t):
    i=0
    f=0
    s1=input()
    s2=input()
    while(i<len(s1)):
        if(s1[i].lower()<s2[i].lower()):
            print(-1)
            f=1
            break
        elif(s1[i].lower()>s2[i].lower()):
            print(1)
            f=1
            break
        else:
            i+=1
    if(f==0):
        print(0)
    n+=1
